{
  "name": "vue3-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts",
    "build-test": "vue-tsc --noEmit && vite build --config ./config/vite.config-test.ts --mode test",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0",
    "ant-design-vue": "~4.2.6",
    "axios": "^1.12.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.18",
    "vue-request": "^2.0.4",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify/vue": "^5.0.0",
    "@types/lodash": "^4.17.20",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^66.5.1",
    "@unocss/preset-icons": "^66.5.1",
    "@unocss/reset": "^66.5.1",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "dayjs": "^1.11.18",
    "eslint": "^9.35.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^8.0.0",
    "less": "^4.4.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "typescript": "~5.8.3",
    "unocss": "^66.5.1",
    "vite": "^7.1.2",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^3.0.5"
  }
}