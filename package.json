{
  "name": "vue3-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts",
    "build-test": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts --mode test",
    "build-offline": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts --mode production-offline",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "prepare": "husky",
    "docker:build": "pnpm docker:remove-image && docker build -t dist .",
    "docker:list-images": "docker images",
    "docker:list-containers": "docker ps -a",
    "docker:run": "docker run -d -p 8888:80 --name dist-web dist",
    "docker:remove-image": "docker rmi -f dist || true",
    "docker:remove-container": "docker rm -f dist-web || true",
    "docker:start-base": "pnpm docker:remove-container && pnpm docker:build && pnpm docker:list-images && pnpm docker:list-containers && pnpm docker:run",
    "docker:start": "pnpm build && pnpm docker:start-base",
    "docker:start-test": "pnpm build-test && pnpm docker:start-base",
    "docker:start-offline": "pnpm build-offline && pnpm docker:start-base"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "ant-design-vue": "~4.2.6",
    "axios": "^1.13.2",
    "echarts": "^6.0.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12",
    "vue-request": "^2.0.4",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/line-md": "^1.2.11",
    "@iconify-json/material-symbols": "^1.2.44",
    "@iconify-json/vscode-icons": "^1.2.33",
    "@iconify/vue": "^5.0.0",
    "@nabla/vite-plugin-eslint": "^2.0.6",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^66.5.1",
    "@unocss/preset-icons": "^66.5.1",
    "@unocss/reset": "^66.5.1",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "colord": "^2.9.3",
    "dayjs": "^1.11.19",
    "eslint": "^9.35.0",
    "eslint-plugin-format": "^1.0.2",
    "gsap": "^3.13.0",
    "husky": "^8.0.3",
    "js-cookie": "^3.0.5",
    "less": "^4.4.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.6",
    "number-precision": "^1.6.0",
    "picocolors": "^1.1.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.2",
    "unocss": "^66.5.6",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.1.7",
    "vite-plugin-compression2": "^2.2.1",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-cropper": "^1.1.4",
    "vue-tsc": "^3.0.5"
  }
}